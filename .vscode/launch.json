{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Run pesquisa.py (source)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/source/pesquisa.py",
            "cwd": "${workspaceFolder}/source",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "justMyCode": true
        },
        {
            "name": "Python: Run as module (python -m source.pesquisa)",
            "type": "python",
            "request": "launch",
            "module": "source.pesquisa",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "justMyCode": true
        }
    ]
}
